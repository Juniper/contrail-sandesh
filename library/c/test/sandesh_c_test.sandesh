enum abc_enum {
    VAL0,
    VAL1,
    VAL2
}

struct abc_inner {
    1:i32 inner_a;
    2:i16 inner_b;
    3:i64 inner_c;
    4:abc_enum inner_d;
    5:u16 inner_e;
    6:u32 inner_f;
    7:u64 inner_g;
    8:xml inner_h;
    9:ipv4 inner_i;
    10:uuid_t inner_j;
    11:ipaddr inner_k;
    12:ipaddr inner_l;
    13:string inner_m;
    14:string inner_n;
}
 
struct abc {
    1:abc_inner nested;     
}

buffer sandesh abc_sandesh {
    1:abc elem;
    2:list<byte> rwinfo;
}
