include "sandesh/library/common/sandesh_dynamic_uve.sandesh"

struct SandeshUVEData {
    1: string name (key="OpserverInfo")
    2: optional bool deleted
    3: optional i32 xyz
}

uve sandesh SandeshUVETest {
    1: SandeshUVEData data
}

struct Config {
    1: string  name (key="")
    2: list<sandesh_dynamic_uve.DynamicElement> elements
    3: optional bool deleted
}

dynamicuve sandesh ConfigUVE {
    1: Config data
}

struct ConfigTest {
    1: string  name (key="")
    2: list<sandesh_dynamic_uve.DynamicElement> elements
    3: optional bool deleted
}

dynamicuve sandesh ConfigTestUVE {
    1: ConfigTest data
}
