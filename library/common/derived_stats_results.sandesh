struct NullResult {
    3: u64 samples 
    5: i32 value
}

struct EWMResult {
    3: u64 samples 
    6: double mean
    7: double stddev
    8: double sigma
}

struct PercentileResult {
    3: u64 samples 
    4: map<string,double> percentiles;
}

struct CategoryResult {
    /** @display_name:Sandesh Client Message Stats*/
    1: map<string, u64> counters (tags=".__key")
}

